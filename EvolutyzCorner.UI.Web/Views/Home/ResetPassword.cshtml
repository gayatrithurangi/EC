    <script type="text/javascript">
        $(document).ready(function () {
            $("#ResetButton").click(function () {
                var newpassword = $("#NewPassword").val();
                $('#myform').validate({
                    rules: {
                        NewPassword: {
                            required: true,
                            regex: /^.*(?=.{8,})(?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[@@#$%&]).*$/

                        },
                        ConfirmNewPassword: {
                            required: true,
                            equalTo: "#NewPassword",
                            regex: /^.*(?=.{8,})(?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[@@#$%&]).*$/

                        },


                    },
                    submitHandler: function (form) {
                        $.ajax({
                            //contentType: 'application/json; charset=utf-8',
                            type: 'POST',
                            url: '/Home/SetNewPassword',
                            data: {
                                'Newpassword': newpassword,

                            },

                            success: function (data) {

                                window.location.href = "/Home/Login";

                            },
                            complete: function () {
                                $('#loading-image').attr("style", "display: none;");
                            },
                            error: function (error) {
                                console.log(error);
                            }

                        });
                        return false;
                    }
                });
                $.validator.addMethod("regex", function (value, element, regexpr) {
                    return regexpr.test(value);
                }, "Password Must contain 1 Capital,1 small,1 number,1 Special Character and length must be 8 and above");
            });
        });

        Element.prototype.backgroundClipPolyfill = function () {
            var a = arguments[0],
                d = document,
                b = d.body,
                el = this;

            function hasBackgroundClip() {
                return b.style.webkitBackgroundClip !== undefined;
            };

            function addAttributes(el, attributes) {
                for (var key in attributes) {
                    el.setAttribute(key, attributes[key]);
                }
            }

            function createSvgElement(tagname) {
                return d.createElementNS('http://www.w3.org/2000/svg', tagname);
            }

            function createSVG() {
                var a = arguments[0],
                    svg = createSvgElement('svg'),
                    pattern = createSvgElement('pattern'),
                    image = createSvgElement('image'),
                    text = createSvgElement('text');

                // Add attributes to elements
                addAttributes(pattern, {
                    'id': a.id,
                    'patternUnits': 'userSpaceOnUse',
                    'width': a.width,
                    'height': a.height
                });

                addAttributes(image, {
                    'width': a.width,
                    'height': a.height
                });
                image.setAttributeNS('http://www.w3.org/1999/xlink', 'xlink:href', a.url);

                addAttributes(text, {
                    'x': 0,
                    'y': 80,
                    'class': a['class'],
                    'style': 'fill:url(#' + a.id + ');'
                });

                // Set text
                text.textContent = a.text;

                // Add elements to pattern
                pattern.appendChild(image);


                svg.appendChild(pattern);
                svg.appendChild(text);

                return svg;
            };

            if (!hasBackgroundClip()) {
                var img = new Image();
                img.onload = function () {
                    var svg = createSVG({
                        'id': a.patternID,
                        'url': a.patternURL,
                        'class': a['class'],
                        'width': this.width,
                        'height': this.height,
                        'text': el.textContent
                    });

                    el.parentNode.replaceChild(svg, el);
                }
                img.src = a.patternURL;
            }
        };

        var element = document.querySelector('.heading-fancy');

        element.backgroundClipPolyfill({
            'patternID': 'mypattern',
            'patternURL': 'https://s3.amazonaws.com/spoonflower/public/design_thumbnails/0537/8621/rrtracerspandex_shop_preview.jpg',
            'class': 'heading-fancy'
        });


    </script>
