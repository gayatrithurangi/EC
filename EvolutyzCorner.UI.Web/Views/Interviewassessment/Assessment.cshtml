    <script type="text/javascript">
        var EXPIRED1 = 0;
        $(document).ready(function () {
            $("#alertmodalforexam.modal").on("show.bs.modal", function () {
                $('body').addClass('modal-color-' + $(this).data('bdcolor'));
            }).modal("show");
            $("#alertmodalforexam.modal").on("hide.bs.modal", function () {
                $('body').removeClass('modal-color-' + $(this).data('bdcolor'));
            });

            $("#Yesforexam").click(function () {
                
                $("#alertmodalforexam.modal").modal("hide");
                var sec = $("#hdnactualtimeinsec").val();
                var countDownDate = new Date();
                console.log(countDownDate.setSeconds(countDownDate.getSeconds() + parseInt(sec)));
                // var countDownDate = new Date("Aug 8, 2020 17:37:25").getTime();
                //var countDownDate = new Date(nowCount).getTime();
                var x = setInterval(function () {
                    var now = new Date().getTime();

                    var distance = countDownDate - now;

                    var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                    var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
                    var seconds = Math.floor((distance % (1000 * 60)) / 1000);
                    document.getElementById("demo").innerHTML = hours + "h "
                        + minutes + "m " + seconds + "s ";

                    if (distance < 0) {
                        EXPIRED1 = 2;
                        clearInterval(x);
                        document.getElementById("demo").innerHTML = "EXPIRED";
                        $("#SaveResult").trigger("click");
                    }
                }, 10);

            });
        });

        window.onbeforeunload = function () {
