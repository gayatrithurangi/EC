        ul.list_options {
            *list-style: inside;
            *list-style-type: upper-alpha;
            list-style: none;
            counter-reset: li;
            font-family: 'Raleway', sans-serif;
            padding: 0;
            font-size: 16px;
            margin-top: 15px;
        }

            ul.list_options.list-flex {
                display: flex;
                flex-wrap: wrap;
            }

            ul.list_options > li {
                /*padding: 10px 0;*/
                padding: 25px 0;
            }

                ul.list_options > li:last-child {
                    padding-bottom: 0;
                }

                ul.list_options > li > .check_opt {
                    display: flex;
                    width: 100%;
                    align-items: center;
                }

                    ul.list_options > li > .check_opt > label.lbl_check > input[type=radio] {
                        width: 100%;
                        opacity: 0.00000001;
                        position: absolute;
                        left: 0;
                        margin: auto;
                        height: 100%;
                    }

                    ul.list_options > li > .check_opt > label.lbl_check {
                        min-height: 20px;
                        cursor: pointer;
                        margin-bottom: 0;
                        position: relative;
                    }

                        ul.list_options > li > .check_opt > label.lbl_check > .helper {
                            position: absolute;
                            top: -4px;
                            left: -4px;
                            cursor: pointer;
                            display: block;
                            font-size: 16px;
                            user-select: none;
                            color: #e7e7e7;
                        }

                            ul.list_options > li > .check_opt > label.lbl_check > .helper:before {
                                content: '';
                                position: absolute;
                                left: 0;
                                top: 0;
                                margin: 5px 10px;
                                width: 18px;
                                height: 18px;
                                transition: transform 0.28s ease;
                                border-radius: 50%;
                                border: 1px solid #717171;
                                background-color: #f7f7f7;
                            }

                            ul.list_options > li > .check_opt > label.lbl_check > .helper:after {
                                content: '';
                                display: block;
                                width: 12px;
                                height: 12px;
                                -webkit-transform: rotate(-45deg) scale(0);
                                -moz-transform: rotate(-45deg) scale(0);
                                -ms-transform: rotate(-45deg) scale(0);
                                transform: rotate(-45deg) scale(0);
                                position: absolute;
                                top: 7.5px;
                                left: 13px;
                                background-color: #20487a;
                                border-radius: 50%;
                            }

                        ul.list_options > li > .check_opt > label.lbl_check > input[type="radio"]:checked ~ .helper::before {
                            color: #183e6d;
                        }

                        ul.list_options > li > .check_opt > label.lbl_check > input[type="radio"]:checked ~ .helper::after {
                            -webkit-transform: rotate(-45deg) scale(1);
                            -moz-transform: rotate(-45deg) scale(1);
                            -ms-transform: rotate(-45deg) scale(1);
                            transform: rotate(-45deg) scale(1);
                        }

                    ul.list_options > li > .check_opt > span {
                        display: block;
                        width: 50%;
                        padding-left: 30px;
                    }

