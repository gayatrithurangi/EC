<div id="Apply_Managerwfhcomments" class="" role="dialog">
    <div class="modal-dialog">

        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Add Comments</h4>
            </div>
            <div class="modal-body" id="">

                <form id="AddModal">
                    <div class="form-group row">
                        <div class="col-md-4">
                            <label>comments</label>
                        </div>
                        <div class="col-md-8">
                            <textarea rows="4" cols="50" id="comments">  </textarea>
                        </div>
                    </div>
                </form>

            </div>
            <div class="modal-footer" id="questions">
                <button type="button" class="btn btn-clr" id="Managerwfhcomments" onclick="Managerwfhcomments()">Ok</button>
                @*<button type="button" class="btn btn-clr1" data-dismiss="modal" id="Cancel"> Cancel</button>*@
            </div>
        </div>

    </div>
</div>

<script>


    var WfhUserid; var userWFHId; var WfhManagerMail; var WfhManagerId; var WfhManagerName; var WfhStartDate; var WfhEndDate; var WfhStatuses;
    WfhUserid      = '@ViewBag.WfhUserid';
    WfhStatuses    = '@ViewBag.WfhStatuses';
    userWFHId      = '@ViewBag.userWFHId';
    WfhManagerMail = '@ViewBag.WfhManagerMail';
    WfhManagerId   = '@ViewBag.WfhManagerId';
    WfhManagerName = '@ViewBag.WfhManagerName';
    WfhStartDate   = '@ViewBag.WfhStartDate';
    WfhEndDate = '@ViewBag.WfhEndDate';
      var meg = '@ViewBag.msg';
    if (meg === "") {
        $('#Apply_Managerwfhcomments').show();
        $("#msgmodal").hide();
    } else {
        $('#Apply_Managerwfhcomments').hide();
        $("#msgmodal").show();
    }
    function Managerwfhcomments() {

        var cmnts = $("#comments").val();
        $.ajax({
            contentType: "application/json; charset=utf-8",
            type: "POST",
            url: "/WrkFrmHomeComments/WorkFromHomeStatus",
            data: JSON.stringify({
                "cmt"     : cmnts,
                "userid"  : WfhUserid,
                "status"  : WfhStatuses,
                "leaveid" : userWFHId,
                "managermail": WfhManagerMail,
                "managerid": WfhManagerId,
                "managername": WfhManagerName,
                "startDate": WfhStartDate,
                "endDate": WfhEndDate
            }),

            success: function (data) {
                $('#Apply_Managerwfhcomments').hide();              
                $("#msgmodal").show();
                $("#mes").text(data);
                //window.location.reload();
               // alert(data);
            },

        });
    }
</script>

