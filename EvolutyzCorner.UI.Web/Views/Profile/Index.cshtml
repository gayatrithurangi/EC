                    'Processing': true,
                    'columns': [
                        { 'data': 'Usr_UserId', "visible": false },
                        { 'data': 'SkillTitle' },
                        { 'data': 'Experience' },
                        { 'data': 'Usr_SkillId', 'visible': false },
                        {
                            "render": function (Usr_SkillId, type, full, meta) {



                                return '<a class="btn btn-icn btn-icn-hide  edit" data-toggle="modal" data-attr="SkillTitle" data-target="#skils_Header" data-userskillid="' + full.Usr_SkillId + '" data-skillID="' + full.SkillId + '" data-id="' + full.Usr_UserId + '" data-STitle="' + full.SkillTitle + '" data-Exp="' + full.Experience + '"  onclick="EditUser(this)" ><i class="fa fa-edit"></i></a><a class="btn btn-icn btn-icn-hide" data-target="#containerDelete" data-toggle="modal"  data-id="' + full.Usr_SkillId + '"  onclick="GetId(this)" "><i class="fa fa-trash"></i></a>';



                                  },

                        },


                    ]
