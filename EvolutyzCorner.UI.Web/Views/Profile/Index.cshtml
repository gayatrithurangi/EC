            }

        .card-content > [class^="edit-"] > fieldset > .after {
            width: 100%;
            height: 2px;
            background: linear-gradient(to right, rgba(101, 56, 0, 0.75) 50%, transparent 50%);
            background-color: rgba(0, 0, 0, 0.3);
            background-size: 200% 100%;
            background-position: 100% 0;
            transition: all 0.6s ease;
        }

        .card-content > [class^="edit-"] > fieldset > input:focus ~ .after,
        .card-content > [class^="edit-"] > fieldset > select:focus ~ .after,
        .card-content > [class^="edit-"] > fieldset > textarea:focus ~ .after {
            background-position: 0 0;
        }

    @@keyframes ripple {
        0% {
            box-shadow: 0px 0px 0px 1px rgba(0, 0, 0, 0);
        }

        50% {
            box-shadow: 0px 0px 0px 15px rgba(0, 0, 0, 0.1);
        }

        100% {
            box-shadow: 0px 0px 0px 15px rgba(0, 0, 0, 0);
        }
