        .card-content > [class^="edit-"] > fieldset > textarea:valid ~ label,
        .card-content > [class^="edit-"] > fieldset > textarea:focus ~ label {
            top: 0;
            transform: scale(0.6, 0.6);
        }

        .card-content > [class^="edit-"] > fieldset > select,
        .card-content > [class^="edit-"] > fieldset > input,
        .card-content > [class^="edit-"] > fieldset > textarea {
            width: 100%;
