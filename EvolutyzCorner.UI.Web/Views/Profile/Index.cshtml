<script type="text/javascript">
     var permissions = '@ViewBag.a';

    if (permissions === "Read") {
        $("#btnAddModel").hide();

        $("#actions").hide();


    }
    else {
        $("#btnAddModel").show();

        $("#actions").show();

    }
    function readURL(input) {
        var allowedExtensions = /(\.jpg|\.jpeg|\.png)$/i;
        var fileInput = document.getElementById('fileUpload');
        //alert(fileInput);
        var filePath = fileInput.value;

        if (!allowedExtensions.exec(filePath)) {

            //$("#ImageUpload").next("span.html").html("Only Images");
            $("#fileUpload").siblings("span.html").show();
            //alert();
            //return false;
            //$("#lblvideoexits").show();
        } else {
            $("#fileUpload").siblings("span.html").hide();
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    //$('#profile-image').css('background-image', 'url(' + e.target.result + ')');
                    $('#profile-image').prop('src', e.target.result);
                    //css('background-image', 'url(' + e.target.result + ')');
                    $('#profile-image').hide();
                    $('#profile-image').fadeIn(650);
                };
                reader.readAsDataURL(input.files[0]);
            }
        }
        
    }
    // Replace source img-404

    $("#profile-image").click(function () {
        $("#fileUpload").trigger('click');
    });
    
    
</script>
