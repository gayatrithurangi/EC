    fieldset > .md-radio {
        display: inline-block;
        margin-top: 5px;
    }

        fieldset > .md-radio > input[type="radio"] {
            display: none;
        }

            fieldset > .md-radio > input[type="radio"]:checked + label:before {
                border-color: #bb6800;
                animation: ripple 0.2s linear forwards;
            }

            fieldset > .md-radio > input[type="radio"]:checked + label:after {
                transform: scale(1);
            }

        fieldset > .md-radio > label {
            display: inline-block;
            color: rgba(0, 0, 0, 0.54);
            height: 20px;
            position: relative !important;
            padding: 0 30px;
            margin-bottom: 0;
            cursor: pointer;
            vertical-align: bottom;
            transform: none !important;
        }

        fieldset > .md-radio > input[type="radio"]:checked + label {
            color: #cc770c;
        }

        fieldset > .md-radio > label:before, fieldset > .md-radio > label:after {
            position: absolute;
            content: '';
            border-radius: 50%;
            transition: all .3s ease;
            transition-property: transform, border-color;
        }

        fieldset > .md-radio > label:before {
            left: 0;
            top: 0;
            width: 20px;
            height: 20px;
            border: 2px solid rgba(0, 0, 0, 0.54);
        }

        fieldset > .md-radio > label:after {
            top: 5px;
            left: 5px;
            width: 10px;
            height: 10px;
            transform: scale(0);
            background: #bb6800;
        }

