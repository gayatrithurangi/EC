    function GetId(id) {
        delskillId = id.getAttribute("data-id");
    }
    function DeleteSkill(delskillId) {

       // var userId = d.getAttribute("data-id");

        $.ajax({

            type: "POST",

            url: "/Profile/DeleteSkills?id=" + delskillId,

            cache: false,
            success: function (res) {

                alert(res);
                window.location.reload();


            },

            error: function (Result) {

                //alert("Error");

            }

        });

    }

    function EditUser(d) {
        //data - userskillid="' + full.Usr_SkillId + '"
        //data - skillID="' + full.SkillId + '"
        //data - id="' + full.Usr_UserId + '"
        //data - STitle="' + full.SkillTitle + '"
        //data - Exp="' + full.Experience + '"

        $("#UpdateSkill").show();
        $("#submit").hide();
        var SkillID = d.getAttribute("data-skillID");

        $("#Skills").val(SkillID);




        var userskillid = d.getAttribute("data-userskillid");
        var Experience = d.getAttribute("data-Exp");

        var seperateExperience = String(Experience).split(".");
        var getFirstVal = seperateExperience[0];
        var getSecondVal = seperateExperience[1];
        $("#yearsOfExperience").val(getFirstVal);
        $("#MonthsOfExperience").val(getSecondVal);

        $("#UpdateSkill").click(function () {
            $("#SkillForm").validate({
                rules: {
                    skills: {
                        required: true
                    },
                    year: {
                        required: true

                    },
                    months: {
                        required: true
                    }
                },
                submitHandler: function (form) {
                    // var userid = usrid;
                    var skill = $("#Skills").val();
                    var yearExp = $("#yearsOfExperience").val();
                    var monthExp = $("#MonthsOfExperience").val();

                    $.ajax({

                        type: "POST",
                        // contentType: "application/json; charset=utf-8",
                        url: "/Profile/updateSkills",
                        data: { SkillsTitle: skill, yearExp: yearExp, monthExp: monthExp, userskillid: userskillid },
                        // dataType: "json",
                        // cache: false,
                        success: function (res) {
                            if (res === "Skill Already Exist") {

                                $("#Skills").after("<label class='error'>" + res + "</label>");
                            } else {
                                $("#Skills").next("label.error").remove();
                                function explode() {
                                    alert(res);
                                }
                                setTimeout(explode, 15);

                                window.location.href = "/Profile/Index";
                            }

                        }

                    });
                }
            });


        });
    }

    function edit()
       {
           $.ajax({

               url: '/Profile/GetProfiletByID?catID=' + UserId,
               type: 'Get',
               success: function (data) {
                   var UsrP_UserProfileID = data.UsrP_UserProfileID;

                   var UsrP_LastName = data.UsrP_LastName;

                   var UsrP_FirstName = data.UsrP_FirstName;

                   var UsrP_DOB = data.UsrP_DOB;

                   var Usrp_DOJ = data.Usrp_DOJ;

                   var UsrP_EmailID = data.UsrP_EmailID;

                   var Usrp_ProfilePicture = data.Usrp_ProfilePicture;

                   var Usrp_MobileNumber = data.Usrp_MobileNumber;

                   var PermanentAddress = data.PermanentAddress;

                   var TemporaryAddress = data.TemporaryAddress;

                   var Marital_Status = data.Marital_Status;

                   var Usr_GenderId = data.Usr_GenderId;

                   var baseUrl = "/uploadimages/Images/";

                   var image = baseUrl + Usrp_ProfilePicture;
                   var image2 = baseUrl + "default.png";


                   if (Usrp_ProfilePicture !== "") {
                       $('#profile-image').show();
                       $('#profile-image').prop('src', image);
                   }
                   else {
                       $('#profile-image').show();
                       $('#profile-image').prop('src', image2);

                   }
                   if (data.UsrP_DOB != null) {
                       var dobDate = new Date(parseInt(UsrP_DOB.substr(6)));
                       $("#Dateofbirth").val(dobDate.format("mm/dd/yyyy"));
                   }

                   if (data.Usrp_DOJ != null) {
                       var dojDate = new Date(parseInt(Usrp_DOJ.substr(6)));
                       $("#Dateofjoin").val(dojDate.format("mm/dd/yyyy"));
                   }

                   // if it is active set the ddl to active else in-active
                   if (Usr_GenderId == 1) {
                       $("#Male").prop("checked", true);
                   } else {
                       $("#Female").prop("checked", true);
                   }
                   if (Marital_Status == "UnMarried") {
                       $("#Single").prop("checked", true);
                   }
                   else if (Marital_Status == "Married"){
                       $("#married").prop("checked", true);
                   }

                   $("#firstname").val(UsrP_FirstName);

                   $("#lastname").val(UsrP_LastName);

                   $("#emailaddr").val(UsrP_EmailID);

                   $("#mobilenumber").val(Usrp_MobileNumber);

                   $("#paddress").val(PermanentAddress);
                   $("#taddress").val(TemporaryAddress);


               },
               error: function () {
                   //alert(Response.text);
               }
           });
    }

