            var status = "UnMarried";
        }
        if (maritalstatus === "2") {
            status = "Married";
        }
        if ($('#mobilenumber').val().length > 10) {
            alert("Mobilenumber must have 10 digits");
            return false;
        }
        var file = document.getElementById("fileUpload").files[0];
        var formData = new FormData();
        formData.append("UsrP_FirstName", $('#firstname').val());
        formData.append("Usrp_ProfilePicture", file);
        formData.append("UsrP_LastName", $('#lastname').val());
        formData.append("UsrP_UserID", UserId);
